/*
	Callback interface to observe changes to a space.
 */
interface ISpaceSaver
{
	void saveChanges();
	ISpace getVersion( in int32 version );
	ISpace getLatestVersion();	
	void setup();
	
	void setModelVersion( in int32 modelVersion);
	void setModel( in IModel model );
	void setSpace( in ISpace space );
};
